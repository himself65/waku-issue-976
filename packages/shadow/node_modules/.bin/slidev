else
  export NODE_PATH="/Users/himself65/waku-issue-976/node_modules/.pnpm/@slidev+cli@0.50.0-beta.4_@babel+parser@7.25.8_@nuxt+kit@3.13.2_rollup@4.24.0_webpack-sources_doyb6nirilczia5mxmumpayadq/node_modules/@slidev/cli/bin/node_modules:/Users/himself65/waku-issue-976/node_modules/.pnpm/@slidev+cli@0.50.0-beta.4_@babel+parser@7.25.8_@nuxt+kit@3.13.2_rollup@4.24.0_webpack-sources_doyb6nirilczia5mxmumpayadq/node_modules/@slidev/cli/node_modules:/Users/himself65/waku-issue-976/node_modules/.pnpm/@slidev+cli@0.50.0-beta.4_@babel+parser@7.25.8_@nuxt+kit@3.13.2_rollup@4.24.0_webpack-sources_doyb6nirilczia5mxmumpayadq/node_modules/@slidev/node_modules:/Users/himself65/waku-issue-976/node_modules/.pnpm/@slidev+cli@0.50.0-beta.4_@babel+parser@7.25.8_@nuxt+kit@3.13.2_rollup@4.24.0_webpack-sources_doyb6nirilczia5mxmumpayadq/node_modules:/Users/himself65/waku-issue-976/node_modules/.pnpm/node_modules:$NODE_PATH"
